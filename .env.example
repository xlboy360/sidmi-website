# EmailJS Configuration
# Copy this file to .env.local and fill in your actual values from EmailJS dashboard

# Your EmailJS Service ID (from Email Services section)
VITE_EMAILJS_SERVICE_ID=your_service_id_here

# Template ID for Wizard admin notifications
VITE_EMAILJS_WIZARD_TEMPLATE_ID=your_wizard_template_id_here

# Template ID for Contact Form admin notifications
VITE_EMAILJS_CONTACT_TEMPLATE_ID=your_contact_template_id_here

# Your EmailJS Public Key (from Account > General)
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here
